{
    "require": {
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/routing": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/yaml": "^6.0",
        "symfony/debug": "^4.4",
        "symfony/dependency-injection": "^6.0",
        "moneyphp/money": "dev-master",
        "symfony/config": "^6.0",
        "monolog/monolog": "^2.8",
        "doctrine/orm": "^2.13",
        "symfony/dotenv": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "OffCut\\RestfulApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OffCut\\RestfulApi\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^7.2",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    }
}
