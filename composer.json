{
    "require": {
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/routing": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/yaml": "^6.0",
        "symfony/debug": "^4.4",
        "symfony/dependency-injection": "^6.0",
        "moneyphp/money": "dev-master",
        "symfony/config": "^6.0",
        "monolog/monolog": "^2.8",
        "symfony/dotenv": "^6.0",
        "symfony/cache": "^6.0",
        "doctrine/annotations": "^2.0",
        "doctrine/orm": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "Xylene\\": ["app/","src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xylene\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^7.2",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    }
}
